
UEq : {A : Set} → A → A → Set
  := \{A} x y. (P : A → Set) → P x → P y

urefl : {A : Set}{x : A} → UEq {A} x x
  := \_ px. px

the : (A : Set) → A → A := \ _ x. x

test1 : Set
  := let m1 : Set * Set -> Set := _;
     let test1 := \A B. the (UEq (m1 (A,B)) A) urefl;
     Set

test2 : Set
  := let m2 : (Set -> Set) -> Set := _;
     let test2 : (Set -> Set) -> UEq _ _
           := \f. the (UEq (m2 (\x. f x)) (f Set)) urefl;
     Set